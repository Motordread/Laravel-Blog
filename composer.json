{
    "name": "fbf/laravel-blog",
    "description": "A Laravel 4 package to add a simple blog to a site",
    "keywords": ["laravel","blog"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/motordread/rss-l5.git"
        }
    ],
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/support": "5.*|6.*",
        "mayconbordin/rss-l5": "dev-master",
        "cviebrock/eloquent-sluggable": "5.*",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/models",
            "src/seeds"
        ],
        "psr-0": {
            "Fbf\\LaravelBlog": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fbf\\LaravelBlog\\LaravelBlogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
